{
  "name": "aurora-ts-core",
  "version": "1.0.9",
  "description": "Aurora npm package starter",
  "author": "Jose Carlos Rodriguez Palac√≠n <carlos.rodriguez.palacin@gmail.com>",
  "license": "MIT",
  "readmeFilename": "README.md",
  "scripts": {
    "start:dev": "tsc -w",
    "build": "rimraf -rf dist && tsc -p tsconfig.json",
    "prepare": "npm run build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "pack": "npm pack && cp $(node -p \"p=require('./package.json');p.name+'-'+p.version+'.tgz'\") ../aurora-app/aurora-ts-core.tgz && npm install ../aurora-app/aurora-ts-core.tgz --prefix ../aurora-app && rm -f $(node -p \"p=require('./package.json');p.name+'-'+p.version+'.tgz'\")"
  },
  "dependencies": {
    "@nestjs/sequelize": "^8.0.0",
    "bcrypt": "^5.0.1",
    "cache-manager": "^3.6.0",
    "clean-deep": "^3.4.0",
    "dayjs": "^1.10.7",
    "mysql2": "^2.3.0",
    "sequelize": "^6.6.5",
    "sequelize-typescript": "^2.1.0",
    "sqlite3": "^5.0.2",
    "uuid-validate": "0.0.3"
  },
  "peerDependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^1.0.1",
    "@nestjs/core": "^8.0.0",
    "@nestjs/graphql": "^9.0.5",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/swagger": "^5.1.5",
    "@nestjs/testing": "8.0.0"
  },
  "devDependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^1.0.1",
    "@nestjs/core": "^8.0.0",
    "@nestjs/cqrs": "^8.0.0",
    "@nestjs/graphql": "^9.0.5",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/sequelize": "^8.0.0",
    "@nestjs/swagger": "^5.1.0",
    "@nestjs/testing": "8.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.1",
    "@types/lodash": "^4.14.175",
    "@types/mime": "^2.0.3",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "apollo-server-express": "^3.3.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "exif-reader": "^1.0.3",
    "graphql": "^15.6.0",
    "graphql-upload": "^12.0.0",
    "jest": "^27.0.6",
    "lodash": "^4.17.21",
    "mime": "^2.5.2",
    "prettier": "^2.3.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.3.0",
    "sequelize": "^6.6.5",
    "sequelize-typescript": "^2.1.2",
    "sharp": "^0.29.1",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
