{
  "name": "@aurora-ts/core",
  "version": "2.2.2",
  "description": "Aurora npm package starter",
  "author": "Jose Carlos Rodriguez Palac√≠n <carlos.rodriguez.palacin@gmail.com>",
  "license": "MIT",
  "readmeFilename": "README.md",
  "scripts": {
    "start:dev": "tsc -w",
    "build": "rimraf -rf dist && tsc -p tsconfig.json",
    "prepare": "npm run build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "pack": "npm pack && cp $(node -p \"p=require('./package.json');p.name+'-'+p.version+'.tgz'\") ../aurora-app/aurora-ts-core.tgz && npm install ../aurora-app/aurora-ts-core.tgz --prefix ../aurora-app && rm -f $(node -p \"p=require('./package.json');p.name+'-'+p.version+'.tgz'\")"
  },
  "dependencies": {
    "@nestjs/sequelize": "^9.0.0",
    "bcrypt": "^5.1.0",
    "cache-manager": "^5.1.7",
    "clean-deep": "^3.4.0",
    "dayjs": "^1.11.7",
    "sequelize": "^6.29.3",
    "sequelize-typescript": "^2.1.5",
    "uuid-validate": "0.0.3"
  },
  "peerDependencies": {
    "@apollo/server": "^4.5.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.0.0",
    "@nestjs/graphql": "^11.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^6.2.1"
  },
  "devDependencies": {
    "@apollo/server": "^4.5.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.0.0",
    "@nestjs/cqrs": "^9.0.3",
    "@nestjs/graphql": "^11.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/sequelize": "^9.0.0",
    "@nestjs/swagger": "^6.2.1",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.8",
    "@types/lodash": "^4.14.191",
    "@types/mime": "^3.0.1",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "exif-reader": "^1.2.0",
    "graphql": "^16.6.0",
    "graphql-upload": "^16.0.2",
    "jest": "28.1.3",
    "lodash": "^4.17.21",
    "mime": "^3.0.0",
    "prettier": "^2.8.4",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "sequelize": "^6.29.3",
    "sequelize-typescript": "^2.1.5",
    "sharp": "^0.31.3",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "publishConfig": {
    "access": "public"
  }
}
